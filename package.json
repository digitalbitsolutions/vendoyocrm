{
  "name": "vendoyo.ios",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "EXPO_ROUTER_APP_ROOT=app expo start --dev-client",
    "start:go": "EXPO_ROUTER_APP_ROOT=app expo start",
    "start:clear": "EXPO_ROUTER_APP_ROOT=app expo start --clear",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "eslint . --ext .js,.jsx ",
    "lint:fix": "eslint . --ext .js,.jsx --fix",
    "format": "prettier --write .",
    "test": "jest",
    "test:quick": "jest --passWithNoTests --silent",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "check": "npm run lint && npm run test",
    "prepare": "husky install",
    "postinstall": "npx pod-install"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-navigation/drawer": "^7.5.0",
    "@react-navigation/native": "^7.1.8",
    "@react-navigation/native-stack": "^7.3.16",
    "expo": "54.0.22",
    "expo-clipboard": "~8.0.7",
    "expo-dev-client": "~6.0.17",
    "expo-image-manipulator": "~14.0.7",
    "expo-image-picker": "~17.0.8",
    "expo-linking": "^8.0.9",
    "expo-local-authentication": "~17.0.7",
    "expo-router": "~6.0.14",
    "expo-secure-store": "~15.0.7",
    "expo-status-bar": "~3.0.8",
    "expo-system-ui": "~6.0.8",
    "formik": "^2.4.6",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-reanimated": "~4.1.3",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-worklets": "^0.6.1",
    "yup": "^1.7.1"
  },
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/eslint-parser": "^7.28.5",
    "@babel/plugin-syntax-flow": "^7.27.1",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@eslint/eslintrc": "^3.3.1",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/react": "~19.2.0",
    "@types/react-native": "^0.72.8",
    "babel-jest": "^30.2.0",
    "babel-preset-expo": "^54.0.7",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-native": "^5.0.0",
    "expo-module-scripts": "^5.0.7",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-expo": "^54.0.13",
    "lint-staged": "^16.2.7",
    "metro-react-native-babel-preset": "^0.77.0",
    "prettier": "^3.6.2",
    "pretty-quick": "^4.2.2",
    "react-test-renderer": "^19.2.0",
    "typescript": "~5.9.2"
  },
  "engines": {
    "node": ">=18 <21",
    "npm": ">=9"
  }
}
